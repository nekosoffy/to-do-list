{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "lf",
    "printWidth": 80,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": true,
    "bracketSpacing": true,
    "insertPragma": false,
    "requirePragma": false,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "eslintIntegration": true,
    "parser": "babel",
    "jsxSingleQuote": true,
    "vueIndentScriptAndStyle": false,
    "singleAttributePerLine": true
}